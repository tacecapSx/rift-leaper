        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Platform behavior

boostTimer--;

if boostDirection != -1 {
    obj_player._hspeed = 0;
    obj_player._vspeed = 0;
    obj_player.boosting = true;
    
    switch(boostDirection) {
        case 0:
            obj_player._hspeed = max(startHspeed, 6);
            obj_player.y = y + yOffset;
        break;
        case 1:
            obj_player._vspeed = min(startVspeed, -6);
            obj_player.x = x + xOffset;
        break;
        case 2:
            obj_player._hspeed = min(startHspeed, -6);
            obj_player.y = y + yOffset;
        break;
        case 3:
            obj_player._vspeed = max(startVspeed, 6);
            obj_player.x = x + xOffset;
        break;
    }
    
    obj_player.hspeed = obj_player._hspeed * timeScale * !obj_portal.swapping;
    obj_player.vspeed = obj_player._vspeed * timeScale * !obj_portal.swapping;
    obj_player.grounded = false;
    
    if !place_meeting(x-obj_player.hspeed,y-obj_player.vspeed,obj_player) {
        var _f = instance_create(obj_player.x,obj_player.y,obj_portalFlash);
        _f.sprite_index = spr_boostFlash;
        _f.image_angle = boostDirection * 90;
        _f.image_xscale = 4;
        _f.image_alpha = 0.4;
        _f.mult = 0.1;
        
        obj_player.boosting = false;
        obj_player.canShoot = true;
        obj_player.jumps = obj_player.maxJumps - 1;
        
        if boostDirection == 1 &amp;&amp; obj_player._vspeed &gt; -7 { // a little exit boost
            obj_player._vspeed *= 1.5;
        }
        
        boostDirection = -1;
    }
}
else if !obj_portal.swapping &amp;&amp; place_meeting(x-obj_player.hspeed,y-obj_player.vspeed,obj_player) {
    if obj_player.x &gt; bbox_right &amp;&amp; sign(obj_player._hspeed) == -1 {
        boostDirection = 2;
    }
    else if obj_player.x &lt; bbox_left &amp;&amp; sign(obj_player._hspeed) == 1 {
        boostDirection = 0;
    }
    else if y + image_yscale * 6 &gt; obj_player.y &amp;&amp; sign(obj_player._vspeed) == 1 {
        boostDirection = 3;
    }
    else if sign(obj_player._vspeed) == -1 {
        boostDirection = 1;
    }
    
    if boostDirection != -1 {
        xOffset = obj_player.x - x;
        yOffset = obj_player.y - y;
        startHspeed = obj_player._hspeed;
        startVspeed = obj_player._vspeed;
        
        if !audio_is_playing(snd_boost) play_sound(snd_boost);
    }
}
</string>
